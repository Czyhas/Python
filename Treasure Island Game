#START GAME


#state_1.1
print(state_1_1)
answer_1_1=input(str("".lower()))
#state_1.2
if answer_1_1 == "left" or answer_1_1 == "l":
    print(mystery_box)
    print(state_1_2)
    answer_1_2=input(str("".lower()))
    #state_2.1
    if answer_1_2 == "yes" or answer_1_2 == "y":
        print(cabinet)
        print(state_2_1)
        answer_2_1=input(str("".lower()))
        #state_3_2    
        if answer_2_1 == "go" or answer_2_1 == "g":
            print(campfire)
            print(state_3_2)
        #state_3.1
        if answer_2_1 == "rest" or answer_2_1 == "r":
            print(fire)
            print(state_3_1)
            answer_3_1=input(str("".lower()))       
            #state_4.1
            if answer_3_1 == "no" or answer_3_1 == "n":
                print(dead)
                print(state_4_1)
            #state_4.2
            if answer_3_1 == "yes" or answer_3_1 == "y":
                print(pic_state_4_2)
                print(state_4_2)
                answer_4_2=input(str("".lower()))
                #state_5.1
                if answer_4_2 == "run" or answer_4_2 == "r":
                    print(pic_state_5_1)
                    print(state_5_1)
                #state_5.2 and 6.1
                if answer_4_2 == "fight" or answer_4_2 == "f":
                    print(swords)
                    print(state_5_2)
                    print(gun)
                    print(state_6_1)
                    answer_6_1=input(str("".lower()))
                    #state_7.1
                    if answer_6_1 == "wait" or answer_6_1 == "w":
                        print(lion)
                        print(state_7_1)
                    #state_7.2
                    if answer_6_1 == "go" or answer_6_1 == "g":
                        print(state_7_2)
                        print(key)
                        print(which_door)
                        answer_which_door=input(str("".lower()))
                        #red
                        if answer_which_door == "red" or answer_which_door == "r":
                            print(fire)
                            print(red_door)
                        #yellow
                        if answer_which_door == "yellow" or answer_which_door == "y":
                            print(win)
                            print(yellow_door)
                        #blue
                        if answer_which_door == "blue" or answer_which_door == "b":
                            print(creature)
                            print(blue_door)
                        #else
                        if answer_which_door != "blue" or answer_which_door != "red" or answer_which_door != "yellow":
                            print(dead)
                            print(none)
                    
                
    #state_2.2
    if answer_1_2 == "no" or answer_1_2 == "n":
        print(cabinet)
        print(state_2_2)
        answer_2_2=input(str("".lower()))
        #state_3.1
        if answer_2_2 == "rest" or answer_2_2 == "r":
            print(fire)
            print(state_3_1)
            answer_3_1=input(str("".lower()))
            #state_4.2
            if answer_3_1 == "yes" or answer_3_1 == "y":
                print(pic_state_4_2)
                print(state_4_2)
                answer_4_2=input(str("".lower()))
                #state_5.1
                if answer_4_2 == "run" or answer_4_2 == "r":
                    print(pic_state_5_1)
                    print(state_5_1)
                #state_5.2 and 6.2
                if answer_4_2 == "fight" or answer_4_2 == "f":
                    print(swords)
                    print(state_5_2)
                    print(dead)
                    print(state_6_2)

        #state_3.3
        if answer_2_2 == "go" or answer_2_2 == "g":
            print(torch)
            print(state_3_3)
            answer_3_3=input(str("".lower()))    
            #state_4.3
            if answer_3_3 == "wait" or answer_3_3 == "w":
                print(pic_knife)
                print(state_4_3)
            #state_4.4
            if answer_3_3 == "run" or answer_3_3 == "r":
                print(face)
                print(state_4_4)
                answer_4_4=input(str("".lower()))
                #state_5.4
                if answer_4_4 == "jump" or answer_4_4 == "j":
                    print(dead)
                    print(state_5_4)
                #state_5.3
                if answer_4_4 == "wait" or answer_4_4 == "w":
                    print(stranger)
                    print(state_5_3)
                    answer_5_3=input(str("".lower()))
                    #state_7.5
                    if answer_5_3 != x:
                        print(dead)
                        print(state_7_5)
                    #state_7.4
                    if answer_5_3 == x:
                        print(state_7_4)
                        print(key)
                        print(which_door)
                        answer_which_door=input(str("".lower()))
                        #red
                        if answer_which_door == "red" or answer_which_door == "r":
                            print(fire)
                            print(red_door)
                        #yellow
                        if answer_which_door == "yellow" or answer_which_door == "y":
                            print(win)
                            print(yellow_door)
                        #blue
                        if answer_which_door == "blue" or answer_which_door == "b":
                            print(creature)
                            print(blue_door)
                        #else
                        if answer_which_door != "blue" or answer_which_door != "red" or answer_which_door != "yellow":
                            print(dead)
                            print(none) 
                    
else:
    print(dead)
    print(state_1_3)




